{
  "name": "newsapp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently -k 'npm:dev:vite' 'neu run --frontend-lib-dev -- --window-enable-inspector'",
    "build": "npm run build:vite && npm run build:neu",
    "dev:neu": "neu run -- --window-enable-inspector",
    "build:neu": "neu build",
    "dev:vite": "vite serve",
    "build:vite": "vite build",
    "postinstall": "neu update",
    "server": "node ./www/src/server/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jsonp": "^0.2.1",
    "node-fetch": "^3.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rss-parser": "^3.13.0",
    "x2js": "^3.4.4"
  },
  "devDependencies": {
    "@neutralinojs/neu": "^9.3.1",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^7.0.0",
    "vite": "^2.8.0"
  }
}
